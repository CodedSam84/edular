<% if current_user %>
	<%= current_user.email %>
	<%= link_to "Account Settings", edit_user_registration_path %>
	<%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
<% else %>
	<%= link_to "Register", new_user_registration_path %>
	<%= link_to "Sign In", user_session_path %>
<% end %>